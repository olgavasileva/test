        :javascript
          init.push(function () {
          	$("#jq-validation-phone").mask("(999) 999-9999");
          	$('#jq-validation-select2').select2({ allowClear: true, placeholder: 'Select a country...' }).change(function(){
          		$(this).valid();
          	});
          	$('#jq-validation-select2-multi').select2({ placeholder: 'Select gear...' }).change(function(){
          		$(this).valid();
          	});

          	// Add phone validator
          	$.validator.addMethod(
          		"phone_format",
          		function(value, element) {
          			var check = false;
          			return this.optional(element) || /^\(\d{3}\)[ ]\d{3}\-\d{4}$/.test(value);
          		},
          		"Invalid phone number."
          	);

          	// Setup validation
          	$("#jq-validation-form").validate({
          		ignore: '.ignore, .select2-input',
          		focusInvalid: false,
          		rules: {
          			'jq-validation-email': {
          			  required: true,
          			  email: true
          			},
          			'jq-validation-password': {
          				required: true,
          				minlength: 6,
          				maxlength: 20
          			},
          			'jq-validation-password-confirmation': {
          				required: true,
          				minlength: 6,
          				equalTo: "#jq-validation-password"
          			},
          			'jq-validation-required': {
          				required: true
          			},
