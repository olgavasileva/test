                  .col-md-4
                    %input.form-control.form-group-margin{:name => "name", :placeholder => "Name", :type => "text"}/
                  .col-md-4
                    %input.form-control.form-group-margin{:name => "email", :placeholder => "Email", :type => "email"}/
                  .col-md-4
                    %input.form-control.form-group-margin{:name => "website", :placeholder => "Website", :type => "url"}/
                / row
