              		if ($b.is(':visible')) {
              			$b.css('display', 'none');
              			$(this).text('Add page breadcrumb');
              		} else {
              			// Display breadcrumb after alerts
              			var $cont = $('#' + self.plugins.alerts.getContainerId());
              			if ($cont.length) {
              				$cont.after($b);
              			} else {
              				$('#content-wrapper').prepend($b);
              			}
              			$b.css('display', 'block');
              			$(this).text('Remove page breadcrumb');
