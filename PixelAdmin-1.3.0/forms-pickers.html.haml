                %input#bs-timepicker-example.form-control{:type => "text"}/
                %hr.panel-wide/
                / As component
                %h6.text-muted.text-semibold.text-xs{:style => "margin:20px 0 10px 0;"} AS COMPONENT
                .input-group.date
                  %input#bs-timepicker-component.form-control{:type => "text"}/
                  %span.input-group-addon
                    %i.fa.fa-clock-o
                %hr.panel-wide/
                / Modal
                %h6.text-muted.text-semibold.text-xs{:style => "margin:20px 0 10px 0;"} MODAL
                .input-group.date
                  %input#bs-timepicker-modal.form-control{:type => "text"}/
                  %span.input-group-addon
                    %i.fa.fa-clock-o
                %hr.panel-wide/
                / Without template
                %h6.text-muted.text-semibold.text-xs{:style => "margin:20px 0 10px 0;"} WITHOUT TEMPLATE
                .has-feedback.date
                  %input#bs-timepicker-w-template.form-control{:type => "text"}/
                  %span.fa.fa-clock-o.form-control-feedback
