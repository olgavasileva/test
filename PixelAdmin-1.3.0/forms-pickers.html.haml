              Bootstrap Timepicker
            / Javascript
            :javascript
              init.push(function () {
              	var options = {
              		minuteStep: 5,
              		orientation: $('body').hasClass('right-to-left') ? { x: 'right', y: 'auto'} : { x: 'auto', y: 'auto'}
              	}
              	$('#bs-timepicker-example').timepicker(options);

              	var options2 = {
              		minuteStep: 1,
              		showSeconds: true,
              		showMeridian: false,
              		showInputs: false,
              		orientation: $('body').hasClass('right-to-left') ? { x: 'right', y: 'auto'} : { x: 'auto', y: 'auto'}
              	}
              	$('#bs-timepicker-component').timepicker(options2);

              	var options3 = {
              		minuteStep: 5,
              		template: 'modal',
              		orientation: $('body').hasClass('right-to-left') ? { x: 'right', y: 'auto'} : { x: 'auto', y: 'auto'}
              	}
              	$('#bs-timepicker-modal').timepicker(options3);

              	var options4 = {
              		template: false,
              		showInputs: false,
              		minuteStep: 5
              	}
              	$('#bs-timepicker-w-template').timepicker(options4);
              });
            / / Javascript
            .panel
              .panel-heading
                %span.panel-title Bootstrap Timepicker
              .panel-body
