              Bootstrap Datepaginator
            / Javascript
            :javascript
              init.push(function () {
              	if (! $('html').hasClass('ie8')) {
              		$('#bs-datepaginator-example').datepaginator();
              		$('#bs-datepaginator-small').datepaginator({ size: 'small' });
              		$('#bs-datepaginator-large').datepaginator({ size: 'large' });
              	} else {
              		$('.ie8-note').attr('style', '');
              	}
              });
            / / Javascript
            .panel
              .panel-heading
                %span.panel-title Bootstrap Datepaginator
              .panel-body
                .note.note-info
                  More info and examples at
                  %a{:href => "http://jondmiles.com/bootstrap-datepaginator/", :target => "_blank"} http://jondmiles.com/bootstrap-datepaginator/
                .note.note-danger.ie8-note{:style => "display:none"} Bootstrap-Datepaginator is disabled because it is extremely slow on IE8.
                #bs-datepaginator-example
                %hr.panel-wide/
                / Sizes
                %h6.text-muted.text-semibold.text-xs{:style => "margin:20px 0 10px 0;"} SIZES
                #bs-datepaginator-small.form-group-margin
                #bs-datepaginator-large
            / /5. $BOOTSTRAP_DATEPAGINATOR
