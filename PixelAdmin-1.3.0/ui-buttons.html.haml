              init.push(function () {
              	$('#outline-switcher').switcher({
              		on_state_content: '<span class="fa fa-check" style="font-size:11px;"></span>',
              		off_state_content: '<span class="fa fa-times" style="font-size:11px;"></span>'
              	});
              	$('#outline-switcher').on($('html').hasClass('ie8') ? "propertychange" : "change", function () {
              		var $panel = $(this).parents('.panel');
              		if ($(this).is(':checked')) {
              			$panel.addClass('panel-danger panel-dark panel-body-colorful');
              			$panel.find('.panel-body .btn').addClass('dark');
              		} else {
              			$panel.removeClass('panel-danger panel-dark panel-body-colorful');
